{
    "name": "matpompili/flarum-latex",
    "description": "Provides LaTeX functionalitty to Flarum",
    "type": "flarum-extension",
    "require": {
        "flarum/core": "^0.1.0-beta.3"
    },
    "extra": {
        "flarum-extension": {
            "title": "LaTeX",
            "icon": {
                "image": "icon.png",
                "backgroundSize": "contain",
                "backgroundRepeat": "no-repeat",
                "backgroundPosition": "center",
                "backgroundColor": "#ddd",
                "color": "#fff"
            }
        }
    }
}
